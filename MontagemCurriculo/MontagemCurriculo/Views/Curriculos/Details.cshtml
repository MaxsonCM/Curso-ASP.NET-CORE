@model MontagemCurriculo.Models.Curriculo

@{
    ViewData["Title"] = "Currículo " + Model.Nome;
}

<div>
    <ul class="collection with-header">
        <li class="collection-header">
            <h5>
                Objetivos <a asp-action="Create" asp-route-id="@Model.CurriculoId" class="btn-floating btn-small blue darken-4"><i class="material-icons">add</i></a>
            </h5>
        </li>
        @await Component.InvokeAsync("Objetivos", Model.CurriculoId)
    </ul>
</div>
